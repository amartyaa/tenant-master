{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["next.env.d.ts", "**/*.js", "**/*.jsx"],
  "exclude": ["node_modules", ".next"]
}
